{
  "name": "my-webpack-library",
  "version": "1.0.2",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/my-webpack-library.js",
  "config": {
    "cmd_webpack": "webpack --config .webpack/config.js"
  },
  "scripts": {
    "echovar": "cross-var echo $npm_package_config_cmd_webpack",
    "build": "%npm_package_config_cmd_webpack% --env dev && %npm_package_config_cmd_webpack% --env build && npm run test",
    "dev": "%npm_package_config_cmd_webpack% --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmunin/webpack-library-starter.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/pmunin/webpack-library-starter",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/webpack": "^4.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cross-var": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "jsdoc-babel": "^0.4.0",
    "mocha": "^5.2.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "yargs": "^11.0.0"
  }
}
